/* pages/contact/contact.wxss */
.contact-list {
  text-align: center;
}
.t1 {
  margin: 20rpx 0;
  font-size: 17px;
  font-weight: 700;
}
.t2 {
  margin: 10rpx 0;
  font-size: 14px;
  letter-spacing: .1em;
}
.contact-img {
  margin: 0 auto;
  width: 80%;
  height: 200px;
  border: 2px solid #0068B7;
  border-radius: 20px;
  overflow: hidden;
}
.contact-img image {
  display: block;
}
.footer {
  display: flex;
  justify-content: space-around;
  margin-top: 20px;
  font-weight: 700;
  text-align: center;
  color: tomato;
}